{
  "name": "@dags/graphs",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexanderLapygin/dags.git"
  },
  "scripts": {
    "prestart": "yarn && yarn bootstrap",
    "boot": "yarn prestart && yarn build",
    "build": "lerna run build --no-private --stream",
    "clean": "lerna clean --yes && rimraf node_modules",
    "test": "lerna run test --no-private --stream",
    "test:coverage": "lerna run test:coverage",
    "lint": "lerna run lint --no-private --stream",
    "bootstrap": "lerna bootstrap --use-workspaces",
    "bundlesize": "bundlesize -c bundlesize.config.json",
    "patch": "node -r esm scripts/patch-workspace.js",
    "release": "lerna publish",
    "release:ci": "lerna publish -y --loglevel verbose",
    "release:next": "lerna publish --force-publish=* --preid next --dist-tag next",
    "publish:next": "lerna publish from-package --dist-tag next"
  },
  "devDependencies": {
    "@babel/cli": "7.10.5",
    "@babel/core": "7.10.5",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/preset-env": "7.10.4",
    "@babel/preset-typescript": "7.10.4",
    "@babel/runtime": "7.10.5",
    "@types/jest": "26.0.5",
    "@types/node": "^14.0.26",
    "@typescript-eslint/eslint-plugin": "3.7.0",
    "@typescript-eslint/parser": "3.7.0",
    "arg": "4.1.3",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "benchmark": "^2.1.4",
    "bundlesize": "0.18.0",
    "chalk": "4.1.0",
    "concurrently": "5.2.0",
    "cross-env": "7.0.2",
    "eslint": "7.5.0",
    "eslint-plugin-jest-dom": "3.1.4",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-testing-library": "3.3.2",
    "esm": "3.2.25",
    "husky": "4.2.5",
    "jest": "26.1.0",
    "lerna": "^3.22.1",
    "lint-staged": "10.2.11",
    "nodemon": "2.0.4",
    "prettier": "2.0.5",
    "ts-jest": "26.1.2",
    "typescript": "^3.9.7"
  },
  "keywords": [
    "data structure",
    "dag",
    "graph",
    "typescript"
  ],
  "author": "https://github.com/AlexanderLapygin",
  "license": "MIT"
}
