{
  "name": "@dags/graphs",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/lib/*",
      "packages/ui-lib/*",
      "examples/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexanderLapygin/dags.git"
  },
  "author": "Alexander Lapygin <alexanderlapygin@gmail.com>",
  "scripts": {
    "prestart": "yarn && yarn bootstrap",
    "boot": "yarn prestart && yarn build",
    "build": "lerna run build --no-private --stream",
    "build:all": "lerna run build --stream",
    "clean": "lerna clean --yes && rimraf node_modules",
    "test": "lerna run test --no-private --stream",
    "test:coverage": "lerna run test:coverage",
    "lint": "lerna run lint --no-private --stream",
    "bootstrap": "lerna bootstrap --use-workspaces",
    "bundlesize": "bundlesize -c bundlesize.config.json",
    "husky": "husky-run lint-staged",
    "versionup-then-release-by-ci": "lerna version --conventional-commits --conventional-graduate",
    "release:changed": "lerna publish from-package",
    "release": "lerna publish",
    "dag": "yarn workspace @dags/dag",
    "dag-base": "yarn workspace @dags/dag-base",
    "dag-local": "yarn workspace @dags/dag-local",
    "uid-local": "yarn workspace @dags/uid-local",
    "uid-uuid": "yarn workspace @dags/uid-uuid"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@babel/runtime": "^7.12.1",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.2",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "arg": "^4.1.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "benchmark": "^2.1.4",
    "bundlesize": "^0.18.0",
    "chalk": "^4.1.0",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest-dom": "^3.2.3",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-testing-library": "^3.9.2",
    "esm": "^3.2.25",
    "husky": "^4.3.0",
    "jest": "^26.6.1",
    "jest-watch-typeahead": "^0.6.1",
    "lerna": "^3.22.1",
    "lint-staged": "^10.4.2",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.2",
    "typescript": "^4.0.5",
    "yarn-deduplicate": "^3.0.0"
  },
  "keywords": [
    "data structure",
    "dag",
    "graph",
    "typescript"
  ],
  "license": "MIT"
}
